<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Movies</name>
    <match>all</match>
	<rule field="genre" operator="isnot">
        <value>family</value>
        <value>children</value>
        <value>christmas</value>
    </rule>
    <order direction="ascending">sorttitle</order>
</smartplaylist>