<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="tvshows">
    <name>Kids Most Played TV Shows</name>
    <match>all</match>
	<rule field="genre" operator="is">
        <value>children</value>
    </rule>
    <limit>50</limit>
	<order direction="descending">playcount</order>
</smartplaylist>